with open('input.txt', 'r') as f:
    lines = f.readlines()
    
def day9pt1(pts):
    areas = []
    for i in range(len(pts)):
        for j in range(i + 1, len(pts)):
            area = (abs(pts[i][0] - pts[j][0]) + 1) * (abs(pts[i][1] - pts[j][1]) + 1)
            areas.append(area)
    print(max(areas))
    
pts = []

for line in lines:
    x, y = map(int, line.split(','))
    pts.append((x, y))

day9pt1(pts)

def day3pt2(arr):
    jolt = 0
    for st in arr:
        jolty = ""
        num = -11
        pos = 0
        nums = []
        for i in st:
            nums.append(int(i))
        while len(jolty) != 12:
            window = nums[pos:num]
            mx = max(window)
            idx = window.index(mx)
            pos = pos + idx
            jolty += str(mx)
            num += 1
    print(jolt)
        
arr = ["382738236416461922142149289"]

day3pt2(arr)

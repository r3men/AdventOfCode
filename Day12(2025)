with open("input.txt", "r") as f:
    lines = f.readlines()

def day12pt1(regions):
    good = 0
    for region in regions:
        dimensions, numbers = region.split(":")
        a, b = map(int, dimensions.split("x"))
        area = a * b
        ze, on, tw, th, fo, fi = map(int, numbers.split())
        s = sum([ze, on, tw, th, fo, fi]) * 9
        if area >= s:   
            good += 1

    print(good)

regions = [line.strip() for line in lines if len(line.strip()) > 3]

day12pt1(regions)
